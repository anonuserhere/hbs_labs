{{#extends "base"}}
{{#block "content"}}
<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>Cost</th>
            <th>Description</th>
            <th>Height(cm)</th>
            <th>Width(cm)</th>
        </tr>

    </thead>
    <tbody>
        {{#each posters}}
        <tr>
            <td>
                {{this.id}}
            </td>
            <td>
                {{this.title}}
            </td>
            <td>
                {{this.cost}}
            </td>
            <td>
                {{this.description}}
            </td>
            <td>{{this.height}}</td>
            <td>{{this.width}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>
{{/block}}
{{/extends}}